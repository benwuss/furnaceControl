<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jl.hl.furnace.model">

	<select id="selectRunningByTimeID" parameterType="string"
		resultType="com.jl.hl.furnace.model.RunningTimeVO">
		select * from valve where running = #{timeID}
	</select>

	<insert id="addRunning" parameterType="com.jl.hl.furnace.model.RunningTimeVO">
		insert into
		running(startTime, endTime, duration,mode)
		values(#{startTime},#{endTime},#{duration},#{mode})
	</insert>


</mapper>